{   
    "api": {
        "log": true,
        "path": "/api"
    },
    "server": {
        "log": true,
        "port": 4000
    },
    "static": {
        "log": false,
        "redirect": false,
        "lastModified": false,
        "resources":  ["/public"]
    },
    "swagger": {
        "log": false,
        "explorer": true,
        "swaggerUrl": null,
        "customSiteTitle": "API",
        "info": "swagger/info.json",
        "paths": "swagger/paths.json",
        "definitions": "swagger/definitions.json",
        "filePath": "swagger/swagger-api-doc.json",
        "customJs": null,
        "customCss": ".swagger-ui .topbar { display: none }"
    },
    "urlencodedParse": {
        "log": false,
        "extended":  false
    },
    "jsonParse": {
        "log": false,
        "strict": false
    },
    "cors": {
        "log": false,
        "origin": ["http://localhost", "127.0.0.1"],
        "exposedHeaders": ["Content-Range", "X-Content-Range"],
        "allowedHeaders": ["Origin", "X-Requested-With", "Content-Type", "Authorization", "Token"]
    }
}
